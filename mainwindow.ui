     <string notr="true">/* Style général pour le QGroupBox */
QGroupBox {
    background-color: transparent;  /* Fond transparent */
    border: 1px solid transparent;  /* Bordure transparente */
    border-radius: 10px;            /* Coins arrondis */
    padding: 10px;                  /* Espacement interne */
}

/* Style pour le QPushButton */
QPushButton {
    background-color: #ebb620; /* Jaune doré */
    color: rgba(0, 0, 0, 220); /* Texte noir avec légère transparence */
    border-radius: 5px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #d9a519; /* Jaune doré plus foncé au survol */
}

QPushButton:pressed {
    padding-left: 5px;
    padding-top: 5px;
    background-color: #b68d17; /* Jaune doré encore plus foncé lors du clic */
}

/* Style pour le QLineEdit */
QLineEdit {
    background-color: rgba(255, 255, 255, 255);
    border: 2px solid rgba(0, 0, 0, 0);
    border-bottom-color: #ebb620; /* Bordure inférieure jaune doré */
    color: rgb(0, 0, 0);
    padding-bottom: 7px;
}
